{
  "name": "redux-http-request-middleware",
  "version": "0.1.0-alpha",
  "description": "Add the HTTP request ability to your own redux actions",
  "homepage": "https://github.com/hk-labs/redux-http-request-middleware#readme",
  "keywords": [
    "react",
    "redux",
    "http",
    "request",
    "middleware",
    "actions",
    "query",
    "fetch",
    "rest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hk-labs/redux-http-request-middleware.git"
  },
  "bugs": {
    "url": "https://github.com/hk-labs/redux-http-request-middleware/issues"
  },
  "main": "dist/lib/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf .nyc_output dist coverage",
    "lint": "eslint src test",
    "test": "cross-env BABEL_ENV=commonjs NODE_ENV=test mocha test/*.spec.js",
    "test:cov": "cross-env NODE_ENV=test nyc npm test",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o dist/lib/index.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o dist/es/index.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/umd/index.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/umd/index.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min"
  },
  "dependencies": {
    "superagent": "^3.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "http-status-codes": "^1.3.0",
    "mocha": "^4.1.0",
    "nock": "^9.1.5",
    "nyc": "^11.4.1",
    "prettier": "^1.9.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-prettier": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.14.0"
  },
  "author": "Dan Kerimdzhanov",
  "license": "MIT"
}
